{
  "name": "velocity-boilerplate",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "build": "NODE_ENV=production node build.js",
    "check-formatting": "prettier --check './src/**/*.ts{,x}'",
    "check-typing": "tsc --noEmit",
    "dev": "NODE_ENV=development node --env-file=.env build.js | pino-pretty",
    "fix-formatting": "prettier --write './src/**/*.ts{,x}'",
    "fix-lint": "eslint --fix './src/**/*.ts{,x}'",
    "lint": "eslint './src/**/*.ts{,x}'",
    "start": "node --enable-source-maps ./.build/production/index.js"
  },
  "dependencies": {
    "@fastify/basic-auth": "6.0.1",
    "@fastify/cookie": "11.0.1",
    "@fastify/formbody": "8.0.1",
    "@fastify/jwt": "9.0.1",
    "@fastify/static": "8.0.2",
    "@formatjs/intl": "2.10.14",
    "@hotwired/turbo": "8.0.12",
    "@nanoweb/jsx": "0.0.19",
    "fastify": "5.1.0",
    "flat": "6.0.1",
    "get-jwks": "9.0.2"
  },
  "devDependencies": {
    "@eslint/js": "9.14.0",
    "@stylistic/eslint-plugin-jsx": "2.10.1",
    "@types/node": "22.9.0",
    "esbuild": "0.24.0",
    "eslint": "9.14.0",
    "eslint-plugin-promise": "7.1.0",
    "globals": "15.12.0",
    "pino-pretty": "12.1.0",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.8",
    "tailwindcss": "3.4.14",
    "typescript": "5.6.3",
    "typescript-eslint": "8.13.0"
  }
}
