{
  "name": "velocity-boilerplate",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "build": "NODE_ENV=production node build.js",
    "check-formatting": "prettier --check './src/**/*.ts{,x}'",
    "check-typing": "tsc --noEmit",
    "dev": "NODE_ENV=development node --env-file=.env build.js | pino-pretty",
    "fix-formatting": "prettier --write './src/**/*.ts{,x}'",
    "fix-lint": "eslint --fix './src/**/*.ts{,x}'",
    "lint": "eslint './src/**/*.ts{,x}'",
    "start": "NODE_ENV=production node --env-file=.env --enable-source-maps ./.build/production/index.js"
  },
  "dependencies": {
    "@fastify/basic-auth": "6.0.1",
    "@fastify/cookie": "11.0.1",
    "@fastify/formbody": "8.0.1",
    "@fastify/jwt": "9.0.1",
    "@fastify/static": "8.0.3",
    "@formatjs/intl": "3.0.4",
    "@hotwired/turbo": "8.0.12",
    "@nanoweb/jsx": "0.0.19",
    "fastify": "5.1.0",
    "flat": "6.0.1",
    "get-jwks": "11.0.0"
  },
  "devDependencies": {
    "@eslint/js": "9.16.0",
    "@stylistic/eslint-plugin-jsx": "2.12.1",
    "@types/node": "22.10.2",
    "esbuild": "0.24.0",
    "eslint": "9.16.0",
    "eslint-plugin-promise": "7.2.1",
    "globals": "15.13.0",
    "pino-pretty": "13.0.0",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "0.6.9",
    "tailwindcss": "3.4.16",
    "typescript": "5.7.2",
    "typescript-eslint": "8.18.0"
  }
}
